{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "trailing-comma": [false],
    "no-console": false,
    "variable-name": [true, "allow-snake-case", "allow-pascal-case"],
    "no-bitwise": false,
    "no-var-requires": false,
    "ban-types": false,
    "prefer-for-of": false
  },
  "rulesDirectory": []
}
